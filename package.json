    "@temporalio/worker": "^1.12.0",
    "@temporalio/workflow": "^1.12.0",
    "@xyflow/react": "^12.0.0",
    "ai": "^6.0.97",
    "autoprefixer": "^10.0.0",
    "better-sqlite3": "^11.0.0",
    "next": "^16.0.0",
    "postcss": "^8.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.0",
    "zod": "^4.3.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["better-sqlite3"]
  },
  "devDependencies": {
